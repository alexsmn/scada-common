<?xml version="1.0" encoding="utf-8" ?>
<opc:ModelDesign
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:opc="http://opcfoundation.org/UA/ModelDesign.xsd"
	xmlns:ua="http://opcfoundation.org/UA/"
	xmlns:uax="http://opcfoundation.org/UA/2008/02/Types.xsd"
	xmlns="http://telecontrol.ru/opc-ua/base/"
	TargetNamespace="http://telecontrol.ru/opc-ua/base/"
>
  <opc:Namespaces>
    <opc:Namespace Name="OpcUa" Prefix="Opc.Ua" XmlNamespace="http://opcfoundation.org/UA/2008/02/Types.xsd">http://opcfoundation.org/UA/</opc:Namespace>
    <opc:Namespace Name="Scada" Prefix="Telecontrol.Scada">http://telecontrol.ru/opc-ua/base/</opc:Namespace>
  </opc:Namespaces>

  <!-- General -->

  <opc:ReferenceType SymbolicName="Creates" BaseType="ua:NonHierarchicalReferences">
    <opc:Description>Objects of target type can be created inside of the source object.</opc:Description>
    <opc:InverseName>CreatableIn</opc:InverseName>
  </opc:ReferenceType>

  <!-- Data Items -->

  <opc:Object SymbolicName="DataItems" TypeDefinition="ua:FolderType">
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>ua:Organizes</opc:ReferenceType>
        <opc:TargetId>ua:ObjectsFolder</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:Object>

  <opc:ObjectType SymbolicName="DataGroupType" BaseType="ua:BaseObjectType">
    <opc:Children>
      <opc:Property SymbolicName="Simulated" DataType="ua:Boolean" />
    </opc:Children>
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>Creates</opc:ReferenceType>
        <opc:TargetId>DataGroupType</opc:TargetId>
      </opc:Reference>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>Creates</opc:ReferenceType>
        <opc:TargetId>DataItems</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:ObjectType>

  <opc:ReferenceType SymbolicName="HasSimulationSignal" BaseType="ua:NonHierarchicalReferences">
    <opc:Description>The Data Item is simulated using the specified Simulation Signal.</opc:Description>
    <opc:InverseName>SimulationSignalOf</opc:InverseName>
  </opc:ReferenceType>

  <opc:ReferenceType SymbolicName="HasHistoricalDatabase" BaseType="ua:NonHierarchicalReferences">
    <opc:Description>The Data Item is historized into the specified Historical Database.</opc:Description>
    <opc:InverseName>HistoricalDatabaseOf</opc:InverseName>
  </opc:ReferenceType>

  <opc:Method SymbolicName="DataItemWriteManualMethodType" ModellingRule="Mandatory" />

  <opc:VariableType SymbolicName="DataItemType" BaseType="ua:BaseDataVariableType">
    <opc:Children>
      <opc:Property SymbolicName="Alias" DataType="ua:String" />
      <opc:Property SymbolicName="Severity" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>1</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="Input1" DataType="ua:String" />
      <opc:Property SymbolicName="Input2" DataType="ua:String" />
      <opc:Property SymbolicName="Output" DataType="ua:String" />
      <opc:Property SymbolicName="OutputCondition" DataType="ua:String" />
      <opc:Property SymbolicName="StalePeriod" DataType="ua:UInt32" />
      <opc:Property SymbolicName="Simulated" DataType="ua:Boolean" />
      <opc:Property SymbolicName="Locked" DataType="ua:Boolean" />
      <opc:Method SymbolicName="WriteManual" TypeDefinition="DataItemWriteManualMethodType"></opc:Method>
    </opc:Children>
    <opc:References>
      <opc:Reference>
        <opc:ReferenceType>HasSimulationSignal</opc:ReferenceType>
        <opc:TargetId>SimulationSignalType</opc:TargetId>
      </opc:Reference>
      <opc:Reference>
        <opc:ReferenceType>HasHistoricalDatabase</opc:ReferenceType>
        <opc:TargetId>HistoricalDatabaseType</opc:TargetId>
      </opc:Reference>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>Creates</opc:ReferenceType>
        <opc:TargetId>DataGroupType</opc:TargetId>
      </opc:Reference>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>Creates</opc:ReferenceType>
        <opc:TargetId>DataItems</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:VariableType>

  <opc:ReferenceType SymbolicName="HasTsFormat" BaseType="ua:NonHierarchicalReferences">
    <opc:Description>The Discrete Item is displayed using the specified TS Format.</opc:Description>
    <opc:InverseName>TsFormatOf</opc:InverseName>
  </opc:ReferenceType>

  <opc:VariableType SymbolicName="DiscreteItemType" BaseType="DataItemType">
    <opc:Children>
      <opc:Property SymbolicName="Inversion" DataType="ua:Boolean" />
    </opc:Children>
    <opc:References>
      <opc:Reference>
        <opc:ReferenceType>HasTsFormat</opc:ReferenceType>
        <opc:TargetId>TsFormatType</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:VariableType>

  <opc:VariableType SymbolicName="AnalogItemType" BaseType="DataItemType">
    <opc:Children>
      <opc:Property SymbolicName="DisplayFormat" DataType="ua:String" />
      <opc:Property SymbolicName="Conversion" DataType="ua:UInt32" />
      <opc:Property SymbolicName="Clamping" DataType="ua:Boolean" />
      <opc:Property SymbolicName="EuLo" DataType="ua:Double">
        <opc:DefaultValue>
          <uax:Double>-100</uax:Double>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="EuHi" DataType="ua:Double">
        <opc:DefaultValue>
          <uax:Double>100</uax:Double>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="IrLo" DataType="ua:Double">
        <opc:DefaultValue>
          <uax:Double>1</uax:Double>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="IrHi" DataType="ua:Double">
        <opc:DefaultValue>
          <uax:Double>65535</uax:Double>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="LimitLo" DataType="ua:Double" />
      <opc:Property SymbolicName="LimitHi" DataType="ua:Double" />
      <opc:Property SymbolicName="LimitLoLo" DataType="ua:Double" />
      <opc:Property SymbolicName="LimitHiHi" DataType="ua:Double" />
      <opc:Property SymbolicName="EngineeringUnits" DataType="ua:String" />
    </opc:Children>
  </opc:VariableType>

  <!-- TS Formats -->

  <opc:Object SymbolicName="TsFormats" TypeDefinition="ua:FolderType">
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>ua:Organizes</opc:ReferenceType>
        <opc:TargetId>ua:ObjectsFolder</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:Object>

  <opc:ObjectType SymbolicName="TsFormatType" BaseType="ua:BaseObjectType">
    <opc:Children>
      <opc:Property SymbolicName="OpenLabel" DataType="ua:String" />
      <opc:Property SymbolicName="CloseLabel" DataType="ua:String" />
      <opc:Property SymbolicName="OpenColor" DataType="ua:UInt32" />
      <opc:Property SymbolicName="CloseColor" DataType="ua:UInt32" />
    </opc:Children>
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>Creates</opc:ReferenceType>
        <opc:TargetId>TsFormats</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:ObjectType>

  <!-- Simulation Signals -->

  <opc:Object SymbolicName="SimulationSignals" TypeDefinition="ua:FolderType">
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>ua:Organizes</opc:ReferenceType>
        <opc:TargetId>ua:ObjectsFolder</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:Object>

  <opc:VariableType SymbolicName="SimulationSignalType" BaseType="ua:BaseVariableType">
    <opc:Children>
      <opc:Property SymbolicName="Type" DataType="ua:UInt32" />
      <opc:Property SymbolicName="Period" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:Double>60000</uax:Double>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="Phase" DataType="ua:UInt32" />
      <opc:Property SymbolicName="UpdateInterval" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:Double>1000</uax:Double>
        </opc:DefaultValue>
      </opc:Property>
    </opc:Children>
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>Creates</opc:ReferenceType>
        <opc:TargetId>SimulationSignals</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:VariableType>

  <!-- Users -->

  <opc:Object SymbolicName="Users" TypeDefinition="ua:FolderType">
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>ua:Organizes</opc:ReferenceType>
        <opc:TargetId>ua:ObjectsFolder</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:Object>

  <opc:ObjectType SymbolicName="UserType" BaseType="ua:BaseObjectType">
    <opc:Children>
      <opc:Property SymbolicName="AccessRights" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>0</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
    </opc:Children>
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>Creates</opc:ReferenceType>
        <opc:TargetId>Users</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:ObjectType>

  <opc:Object SymbolicName="RootUser" TypeDefinition="UserType">
    <opc:BrowseName>root</opc:BrowseName>
    <opc:Children>
      <opc:Property SymbolicName="AccessRights">
        <opc:Value>
          <uax:UInt32>3</uax:UInt32>
        </opc:Value>
      </opc:Property>
    </opc:Children>
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>ua:Organizes</opc:ReferenceType>
        <opc:TargetId>Users</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:Object>

  <!-- Historical Databases -->

  <opc:Object SymbolicName="HistoricalDatabases" TypeDefinition="ua:FolderType">
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>ua:Organizes</opc:ReferenceType>
        <opc:TargetId>ua:ObjectsFolder</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:Object>

  <opc:ObjectType SymbolicName="HistoricalDatabaseType" BaseType="ua:BaseObjectType">
		<opc:Children>
			<opc:Property SymbolicName="Depth" DataType="ua:UInt32">
				<opc:DefaultValue>
					<uax:UInt32>1</uax:UInt32>
				</opc:DefaultValue>
			</opc:Property>
      <opc:Variable SymbolicName="WriteValueDuration" TypeDefinition="ua:BaseDataVariableType" DataType="ua:UInt32" />
      <opc:Variable SymbolicName="PendingTaskCount" TypeDefinition="ua:BaseDataVariableType" DataType="ua:UInt32" />
      <opc:Variable SymbolicName="EventCleanupDuration" TypeDefinition="ua:BaseDataVariableType" DataType="ua:UInt32" />
      <opc:Variable SymbolicName="ValueCleanupDuration" TypeDefinition="ua:BaseDataVariableType" DataType="ua:UInt32" />
		</opc:Children>
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>Creates</opc:ReferenceType>
        <opc:TargetId>HistoricalDatabases</opc:TargetId>
      </opc:Reference>
    </opc:References>
	</opc:ObjectType>

  <opc:Object SymbolicName="SystemDatabase" TypeDefinition="HistoricalDatabaseType">
    <opc:BrowseName>System</opc:BrowseName>
    <opc:Children>
      <opc:Property SymbolicName="Depth">
        <opc:Value>
          <uax:UInt32>30</uax:UInt32>
        </opc:Value>
      </opc:Property>
    </opc:Children>
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>ua:Organizes</opc:ReferenceType>
        <opc:TargetId>HistoricalDatabases</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:Object>

  <!-- Devices -->

  <opc:Object SymbolicName="Devices" TypeDefinition="ua:FolderType">
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>ua:Organizes</opc:ReferenceType>
        <opc:TargetId>ua:ObjectsFolder</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:Object>

  <opc:Method SymbolicName="DeviceInterrogateMethodType" ModellingRule="Mandatory">
    <!-- opc:InputArguments>
      <opc:Argument Name="Name" DataType="ua:String">
        <opc:Description>The name of the controller to create.</opc:Description>
      </opc:Argument>
    </opc:InputArguments>
    <opc:OutputArguments>
      <opc:Argument Name="NodeId" DataType="ua:NodeId">
        <opc:Description>The NodeId for the controller that was created</opc:Description>
      </opc:Argument>
    </opc:OutputArguments -->
  </opc:Method>

  <opc:Method SymbolicName="DeviceSyncClockMethodType" ModellingRule="Mandatory" />

  <opc:Method SymbolicName="DeviceSelectMethodType" ModellingRule="Mandatory" />

  <opc:Method SymbolicName="DeviceWriteMethodType" ModellingRule="Mandatory" />

  <opc:Method SymbolicName="DeviceWriteParamMethodType" ModellingRule="Mandatory" />

  <opc:Method SymbolicName="DeviceInterrogateChannelMethodType" ModellingRule="Mandatory" />

  <opc:ObjectType SymbolicName="DeviceType" BaseType="ua:BaseObjectType">
    <opc:Children>
      <opc:Property SymbolicName="Disabled" DataType="ua:Boolean" />
      <opc:Variable SymbolicName="Online" TypeDefinition="ua:BaseDataVariableType" DataType="ua:Boolean" />
      <opc:Variable SymbolicName="Enabled" TypeDefinition="ua:BaseDataVariableType" DataType="ua:Boolean" />
      <opc:Method SymbolicName="Interrogate" TypeDefinition="DeviceInterrogateMethodType"></opc:Method>
      <opc:Method SymbolicName="InterrogateChannel" TypeDefinition="DeviceInterrogateChannelMethodType"></opc:Method>
      <opc:Method SymbolicName="SyncClock" TypeDefinition="DeviceSyncClockMethodType"></opc:Method>
      <opc:Method SymbolicName="Select" TypeDefinition="DeviceSelectMethodType"></opc:Method>
      <opc:Method SymbolicName="Write" TypeDefinition="DeviceWriteMethodType"></opc:Method>
      <opc:Method SymbolicName="WriteParam" TypeDefinition="DeviceWriteParamMethodType"></opc:Method>
    </opc:Children>
  </opc:ObjectType>

  <opc:ObjectType SymbolicName="LinkType" BaseType="DeviceType">
    <opc:Children>
      <opc:Property SymbolicName="Transport" DataType="ua:String" />
      <opc:Variable SymbolicName="ConnectCount" TypeDefinition="ua:BaseDataVariableType" DataType="ua:UInt32" />
      <opc:Variable SymbolicName="ActiveConnections" TypeDefinition="ua:BaseDataVariableType" DataType="ua:UInt32" />
      <opc:Variable SymbolicName="MessagesOut" TypeDefinition="ua:BaseDataVariableType" DataType="ua:UInt32" />
      <opc:Variable SymbolicName="MessagesIn" TypeDefinition="ua:BaseDataVariableType" DataType="ua:UInt32" />
      <opc:Variable SymbolicName="BytesOut" TypeDefinition="ua:BaseDataVariableType" DataType="ua:UInt32" />
      <opc:Variable SymbolicName="BytesIn" TypeDefinition="ua:BaseDataVariableType" DataType="ua:UInt32" />
    </opc:Children>
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>Creates</opc:ReferenceType>
        <opc:TargetId>Devices</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:ObjectType>

  <opc:ObjectType SymbolicName="DeviceWatchEventType" BaseType="ua:BaseEventType">
    <opc:Description>Device watch event.</opc:Description>
    <!-- opc:Children>
      <opc:Property SymbolicName="Temperature" DataType="ua:Double"></opc:Property>
      <opc:Property SymbolicName="State" DataType="ua:Int32" AccessLevel="ReadWrite" />
    </opc:Children -->
  </opc:ObjectType>

  <!-- IEC-60870 Devices -->

  <opc:ObjectType SymbolicName="Iec60870LinkType" BaseType="LinkType">
    <opc:Children>
      <opc:Property SymbolicName="Protocol" DataType="ua:UInt32" />
      <opc:Property SymbolicName="Mode" DataType="ua:UInt32" />
      <opc:Property SymbolicName="SendQueueSize" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>12</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="ReceiveQueueSize" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>8</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="ConnectTimeout" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>30</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="ConfirmationTimeout" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>5</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="TerminationTimeout" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>20</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="DeviceAddressSize" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>2</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="COTSize" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>2</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="InfoAddressSize" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>3</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="DataCollection" DataType="ua:Boolean">
        <opc:DefaultValue>
          <uax:Boolean>true</uax:Boolean>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="SendRetryCount" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>0</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="CRCProtection" DataType="ua:Boolean">
        <opc:DefaultValue>
          <uax:Boolean>false</uax:Boolean>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="SendTimeout" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>5</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="ReceiveTimeout" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>10</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="IdleTimeout" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>30</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="AnonymousMode" DataType="ua:Boolean">
        <opc:DefaultValue>
          <uax:Boolean>false</uax:Boolean>
        </opc:DefaultValue>
      </opc:Property>
    </opc:Children>
  </opc:ObjectType>

  <opc:ObjectType SymbolicName="Iec60870DeviceType" BaseType="DeviceType">
    <opc:Children>
      <opc:Property SymbolicName="Address" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>1</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="LinkAddress" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>1</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="StartupInterrogation" DataType="ua:Boolean">
        <opc:DefaultValue>
          <uax:Boolean>true</uax:Boolean>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="InterrogationPeriod" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>0</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="StartupClockSync" DataType="ua:Boolean">
        <opc:DefaultValue>
          <uax:Boolean>true</uax:Boolean>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="ClockSyncPeriod" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>0</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <!-- TODO: Array -->
      <opc:Property SymbolicName="InterrogationPeriodGroup1" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup2" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup3" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup4" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup5" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup6" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup7" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup8" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup9" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup10" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup11" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup12" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup13" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup14" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup15" DataType="ua:UInt32" />
      <opc:Property SymbolicName="InterrogationPeriodGroup16" DataType="ua:UInt32" />
    </opc:Children>
    <opc:References>
      <opc:Reference IsInverse="true">
        <opc:ReferenceType>Creates</opc:ReferenceType>
        <opc:TargetId>Iec60870LinkType</opc:TargetId>
      </opc:Reference>
    </opc:References>
  </opc:ObjectType>

  <!-- MODBUS Devices -->

  <opc:DataType SymbolicName="ModbusLinkModeType" BaseType="ua:Enumeration">
    <opc:Fields>
      <opc:Field Name="Polling" />
      <opc:Field Name="Transmission" />
      <opc:Field Name="Listening" />
    </opc:Fields>
  </opc:DataType>

  <opc:ObjectType SymbolicName="ModbusLinkType" BaseType="LinkType">
    <opc:Children>
      <opc:Property SymbolicName="Mode" DataType="ModbusLinkModeType" />
    </opc:Children>
  </opc:ObjectType>

  <opc:ObjectType SymbolicName="ModbusDeviceType" BaseType="DeviceType">
    <opc:Children>
      <opc:Property SymbolicName="Address" DataType="ua:UInt32">
        <opc:DefaultValue>
          <uax:UInt32>1</uax:UInt32>
        </opc:DefaultValue>
      </opc:Property>
      <opc:Property SymbolicName="SendRetryCount" DataType="ua:UInt32">
