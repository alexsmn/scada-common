<xs:schema
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:ua="http://opcfoundation.org/UA/2008/02/Types.xsd"
  xmlns:tns="http://telecontrol.ru/opc-ua/base/"
  targetNamespace="http://telecontrol.ru/opc-ua/base/"
  elementFormDefault="qualified"
>
  <xs:import namespace="http://opcfoundation.org/UA/2008/02/Types.xsd" />

  <xs:simpleType  name="ModbusLinkModeType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Polling_1" />
      <xs:enumeration value="Transmission_2" />
      <xs:enumeration value="Listening_3" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="ModbusLinkModeType" type="tns:ModbusLinkModeType" />

  <xs:complexType name="ListOfModbusLinkModeType">
    <xs:sequence>
      <xs:element name="ModbusLinkModeType" type="tns:ModbusLinkModeType" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ListOfModbusLinkModeType" type="tns:ListOfModbusLinkModeType" nillable="true"></xs:element>

</xs:schema>