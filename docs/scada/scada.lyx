\end_layout

\begin_layout Itemize
cos(x) - 
\begin_inset Formula $\cos x$
\end_inset


\end_layout

\begin_layout Itemize
tan(x) - 
\begin_inset Formula $\tan x$
\end_inset


\end_layout

\begin_layout Itemize
asin(x) - 
\begin_inset Formula $\arcsin x$
\end_inset


\end_layout

\begin_layout Itemize
acos(x) - 
\begin_inset Formula $\arccos x$
\end_inset


\end_layout

\begin_layout Itemize
atan(x) - 
\begin_inset Formula $\arctan x$
\end_inset


\end_layout

\begin_layout Itemize
atan2(y, x) - 
\begin_inset Formula $\arctan\frac{y}{x}$
\end_inset


\end_layout

\begin_layout Itemize
abs(x) - 
\begin_inset Formula $\left\Vert x\right\Vert $
\end_inset


\end_layout

\begin_layout Itemize
not(x) - отрицание:
\begin_inset Formula $\begin{cases}
0 & x\neq0\\
1 & x=0
\end{cases}$
\end_inset


\end_layout

\begin_layout Itemize
sqrt(x) - 
\begin_inset Formula $\sqrt{x}$
\end_inset


\end_layout

\begin_layout Itemize
sign(x) - знак: 
\begin_inset Formula $\begin{cases}
-1 & x<0\\
0 & x=0\\
1 & x>0
\end{cases}$
\end_inset

 
\end_layout

\begin_layout Itemize
min(x1, x2, ...) - 
\begin_inset Formula $\min x1,x2,...$
\end_inset

 (произвольное число параметров)
\end_layout

\begin_layout Itemize
max(x1, x2, ...) - 
\begin_inset Formula $\max x1,x2,...$
\end_inset


\end_layout

\begin_layout Itemize
